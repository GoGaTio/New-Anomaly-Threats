<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[@Name="NAT_RustedBullet"]/projectile</xpath>
		<value>
			<thingClass>CombatExtended.BulletCE</thingClass>
			<projectile Class="CombatExtended.ProjectilePropertiesCE">
				<damageDef>Bullet</damageDef>
			</projectile>
		</value>
	</Operation>

	<!-- ========== Misc ========== -->

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="NAT_Bullet_RustedSmall"]/projectile</xpath>
		<value>
			<projectile Class="CombatExtended.ProjectilePropertiesCE">
				<damageAmountBase>12</damageAmountBase>
				<armorPenetrationSharp>3.5</armorPenetrationSharp>
				<armorPenetrationBlunt>10.860</armorPenetrationBlunt>
				<speed>67</speed>
			</projectile>
		</value>
	</Operation>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ThingDef[defName="NAT_Bullet_RustedSmallStop"]</xpath>
	</Operation>

	<Operation Class="PatchOperationReplace">
