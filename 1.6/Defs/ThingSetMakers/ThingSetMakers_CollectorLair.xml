<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingSetMakerDef>
    <defName>NAT_CollectoirLairCase</defName>
    <root Class="ThingSetMaker_Sum">
      <options>

        <li>
          <chance>0.6</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Silver</li>
                </thingDefs>
              </filter>
              <countRange>200~700</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.3</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Silver</li>
                </thingDefs>
              </filter>
              <countRange>100~1000</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.05</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Silver</li>
                </thingDefs>
              </filter>
              <countRange>100~1000</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.4</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <filter>
                <categories>
                  <li>ResourcesRaw</li>
                </categories>
                <disallowCheaperThan>7</disallowCheaperThan>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                  <li>NonStandardReward</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.25</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <totalMarketValueRange>150~200</totalMarketValueRange>
              <filter>
                <categories>
                  <li>ResourcesRaw</li>
                </categories>
                <disallowCheaperThan>7</disallowCheaperThan>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                  <li>NonStandardReward</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.25</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>300</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Weapons</li>
                </categories>
                <disallowedThingDefs>
                  <li>WoodLog</li>
                  <li>ElephantTusk</li>
                </disallowedThingDefs>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.2</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>300</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Weapons</li>
                </categories>
                <disallowedThingDefs>
                  <li>WoodLog</li>
                  <li>ElephantTusk</li>
                </disallowedThingDefs>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.25</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>300</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Apparel</li>
                </categories>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.3</chance>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <maxThingMarketValue>300</maxThingMarketValue>
              <filter>
                <categories>
                  <li>Apparel</li>
                </categories>
                <thingSetMakerTagsToDisallow>
                  <li>RewardStandardHighFreq</li>
                  <li>RewardStandardCore</li>
                </thingSetMakerTagsToDisallow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>
        
        <!-- Artifacts -->
        <li>
          <chance>0.1</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <categories>
                  <li>Artifacts</li>
                </categories>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.2</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <categories>
                  <li>Artifacts</li>
                </categories>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li>
          <chance>0.1</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <categories>
                  <li>Artifacts</li>
                </categories>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <li MayRequire="Ludeon.RimWorld.Odyssey">
          <chance>0.1</chance>
          <thingSetMaker Class="ThingSetMaker_UniqueWeapon" />
        </li>

        <li MayRequire="Ludeon.RimWorld.Odyssey">
          <chance>0.05</chance>
          <thingSetMaker Class="ThingSetMaker_UniqueWeapon" />
        </li>

        <!-- Misc cool items -->
        <li>
          <chance>0.15</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <qualityGenerator>Reward</qualityGenerator>
              <filter>
                <thingDefs>
                  <li>Apparel_PowerArmor</li>
                  <li>Apparel_PowerArmorHelmet</li>
                  <li>Gun_ChargeRifle</li>
                  <li>Gun_ChargeLance</li>
                  <li>Gun_AssaultRifle</li>
                  <li>TechprofSubpersonaCore</li>
                  <li>ArchotechEye</li>
                  <li>ArchotechArm</li>
                  <li>ArchotechLeg</li>
                  <li>PowerClaw</li>
                  <li>BionicEye</li>
                  <li>BionicArm</li>
                  <li>BionicLeg</li>
                  <li>BionicSpine</li>
                  <li>BionicHeart</li>
                  <li>Joywire</li>
                  <li>Painstopper</li>
                  <li>Shell_AntigrainWarhead</li>
                  <li>MechSerumHealer</li>
                  <li>MechSerumResurrector</li>
                  <li>FlatscreenTelevision</li>
                  <li>MegascreenTelevision</li>
                  <li>SculptureGrand</li>
                </thingDefs>
                <thingSetMakerTagsToAllow>
                  <li>SingleUseWeapon</li>
                  <li>SkillNeurotrainer</li>
                </thingSetMakerTagsToAllow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Techprints -->
        <!-- note this is intended to ignore player needs entirely, it gives entirely random techprints -->
        <li>
          <chance>0.05</chance>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingSetMakerTagsToAllow>
                  <li>Techprint</li>
                </thingSetMakerTagsToAllow>
              </filter>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Luciferium -->
        <li>
          <chance>0.15</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Luciferium</li>
                </thingDefs>
              </filter>
              <countRange>5~20</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Advanced components -->
        <li>
          <chance>0.15</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentSpacer</li>
                </thingDefs>
              </filter>
              <countRange>1~4</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Ultratech meds -->
        <li>
          <chance>0.15</chance>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>MedicineUltratech</li>
                </thingDefs>
              </filter>
              <countRange>2~8</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>

</Defs>
