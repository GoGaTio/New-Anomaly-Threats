<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingSetMakerDef>
    <defName>NAT_RustedBox</defName>
    <root Class="ThingSetMaker_RandomOption">
      <options>

        <!-- Weapons -->
        <li>
          <weight>0.5</weight> 
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingSetMakerTagsToAllow>
                  <li>NAT_RustedWeapon</li>
                </thingSetMakerTagsToAllow>
              </filter>
              <countRange>1~1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Activators -->
        <li>
          <weight>0.8</weight> 
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingSetMakerTagsToAllow>
                  <li>NAT_RustedActivator</li>
                </thingSetMakerTagsToAllow>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Serums -->
        <li>
          <weight>0.9</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <tradeTagsToAllow>
                  <li>Serum</li>
                </tradeTagsToAllow>
              </filter>
              <countRange>2~4</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Resources -->
        <li>
          <weight>0.5</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Bioferrite</li>
                </thingDefs>
              </filter>
              <countRange>7~23</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Leather_Dread</li>
                </thingDefs>
              </filter>
              <countRange>20~40</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>0.7</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Meat_Twisted</li>
                </thingDefs>
              </filter>
              <countRange>30~45</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>0.3</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Silver</li>
                </thingDefs>
              </filter>
              <countRange>10~70</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>0.1</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>NAT_PainClot</li>
                </thingDefs>
              </filter>
              <countRange>2~5</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Firefoam -->
        <li>
          <weight>0.1</weight>
          <thingSetMaker Class="ThingSetMaker_Count">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Apparel_FirefoampopPack</li>
                  <li>FirefoamPopper</li>
                </thingDefs>
              </filter>
              <countRange>1~1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

        <!-- Harbinger seed -->
        <li>
          <weight>0.15</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>HarbingerSeed</li>
                </thingDefs>
              </filter>
              <countRange>1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>

      </options>
    </root>
  </ThingSetMakerDef>

</Defs>
