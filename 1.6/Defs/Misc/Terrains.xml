    <defName>NAT_RustedFloor</defName>
    <label>rusted surface</label>
    <description>TODO</description>
    <pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainFadeRoughLinearBurn</pollutionShaderType>
    <pollutionOverlayTexturePath>Terrain/Surfaces/PollutionFloorSmooth</pollutionOverlayTexturePath>
    <pollutionColor>(1, 1, 1, 0.8)</pollutionColor>
    <pollutionOverlayScale>(0.75, 0.75)</pollutionOverlayScale>
    <canEverTerraform>false</canEverTerraform>
    <spaceEdgeGraphicData Inherit="False" MayRequire="Ludeon.RimWorld.Odyssey">
      <OShapeTexPath>NAT_RustedPlatformBorderInside_OShape</OShapeTexPath>
      <UShapeTexPath>NAT_RustedPlatformBorderInside_UShape</UShapeTexPath>
      <CornerInnerTexPath>NAT_RustedPlatformBorderInside_CornerInner</CornerInnerTexPath>
      <CornerOuterTexPath>NAT_RustedPlatformBorderInside_CornerOuter</CornerOuterTexPath>
      <FlatTexPath>NAT_RustedPlatformBorderInside_Flat</FlatTexPath>

      <LoopLeftTexPath>NAT_RustedPlatformEdge_Left</LoopLeftTexPath>
      <LoopRightTexPath>NAT_RustedPlatformEdge_Right</LoopRightTexPath>
      <LoopSingleTexPath>NAT_RustedPlatformEdge_Single</LoopSingleTexPath>

      <LoopTexPaths>
        <li>NAT_RustedPlatformEdge_Loop1</li>
        <li>NAT_RustedPlatformEdge_Loop2</li>
        <li>NAT_RustedPlatformEdge_Loop3</li>
        <li>NAT_RustedPlatformEdge_Loop4</li>
        <li>NAT_RustedPlatformEdge_Loop5</li>
      </LoopTexPaths>
    </spaceEdgeGraphicData>
    <texturePath>Terrain/NAT_RustedFloor</texturePath>
    <renderPrecedence>393</renderPrecedence>
    <fertility>0</fertility>
    <layerable>false</layerable>
    <designationCategory />
    <pathCost>1</pathCost>
    <edgeType>FadeRough</edgeType>
    <statBases>
      <Beauty>-10</Beauty>
      <Cleanliness>1</Cleanliness>
      <Flammability>0</Flammability>
      <CleaningTimeFactor>0.8</CleaningTimeFactor>
    </statBases>
  </TerrainDef>

