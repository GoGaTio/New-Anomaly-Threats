<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <ThingDef Abstract="True" Name="NAT_RustedCrateBase" ParentName="NAT_RustedBuildingBase">
    <pathCost>50</pathCost>
    <castEdgeShadows>true</castEdgeShadows>
    <hasInteractionCell>false</hasInteractionCell>
    <fillPercent>0.4</fillPercent>
    <tickerType>Normal</tickerType>
    <drawerType>MapMeshAndRealTime</drawerType>
    <thingClass>Building_Crate</thingClass>
    <canOverlapZones>false</canOverlapZones>
    <building>
      <ai_chillDestination>true</ai_chillDestination>
      <paintable>false</paintable>
      <ai_neverTrashThis>true</ai_neverTrashThis>
      <deconstructible>false</deconstructible>
      <claimable>false</claimable>
      <neverBuildable>true</neverBuildable>
      <destroyEffecter>NAT_RustedSoldierDeath</destroyEffecter>
      <openingStartedSound>Building_Deconstructed</openingStartedSound>
      <isInert>true</isInert>
    </building>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Flammability>1</Flammability>
      <Beauty>-2</Beauty>
    </statBases>
    <passability>PassThroughOnly</passability>
    <blockWind>false</blockWind>
    <rotatable>false</rotatable>
    <staticSunShadowHeight>0.4</staticSunShadowHeight>
    <noRightClickDraftAttack>true</noRightClickDraftAttack>
  </ThingDef>

  <ThingDef ParentName="NAT_RustedCrateBase">
    <defName>NAT_RustedBox</defName>
    <label>rusted crate</label>
    <description>A rusted bioferrite crate, may content something valuable.</description>
    <graphicData>
      <texPath>Things/Building/NAT_RustedCrates/NAT_RustedBox</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3, 2)</drawSize>
    </graphicData>
    <size>(2,1)</size>
	<rotatable>true</rotatable>
    <comps>
      <li Class="CompProperties_EmptyStateGraphic">
        <graphicData>
          <texPath>Things/Building/NAT_RustedCrates/NAT_RustedBox_Open</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>(3,2)</drawSize>
        </graphicData>
      </li>
      <li Class="CompProperties_LootSpawn">
        <contents>NAT_RustedBox</contents>
      </li>
    </comps>
  </ThingDef> 

