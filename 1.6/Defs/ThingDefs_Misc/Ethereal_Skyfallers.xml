<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="SkyfallerBase">
    <defName>NAT_RustedTrooperIncoming</defName>
    <label>rusted trooper (incoming)</label>
    <thingClass>NAT.Skyfaller_RustedChunk</thingClass>
    <graphicData>
      <texPath>Things/NAT_RustedTrooperIncoming</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(3, 3)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>ConstantSpeed</movementType>
      <shadowSize>(1, 1)</shadowSize>
      <ticksToImpactRange>300~420</ticksToImpactRange>
      <anticipationSound>WarpedObelisk_Fall</anticipationSound>
      <anticipationSoundTicks>360</anticipationSoundTicks>
      <impactSound>WarpedObelisk_Impact</impactSound>
      <rotateGraphicTowardsDirection>true</rotateGraphicTowardsDirection>
      <motesPerCell>0</motesPerCell>
      <explosionRadius>1.9</explosionRadius>
    </skyfaller>
  </ThingDef>

